{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test-ci": "jest --ci",
    "test-update": "jest --updateSnapshot  --coverage=false",
    "lint": "eslint --color --ext .js,.jsx src App.js",
    "lint-fix": "npm run lint -- --fix",
    "lint-quiet": "npm run lint -- --quiet",
    "format": "prettier \"src/**/*{.js,.jsx}\" \"__tests__/**/*{.js,.jsx}\" App.js",
    "format-write": "npm run format -- --write",
    "format-check": "npm run format -- --check",
    "clean-node": "rimraf node_modules"
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "0.2.8",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/stack": "^6.3.11",
    "expo": "^47.0.13",
    "expo-barcode-scanner": "~12.1.0",
    "expo-camera": "~13.1.0",
    "expo-constants": "~14.0.2",
    "expo-face-detector": "~12.0.0",
    "expo-file-system": "~15.1.1",
    "expo-media-library": "~15.0.0",
    "expo-permissions": "~14.0.0",
    "expo-screen-orientation": "~5.0.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-reanimated": "~2.12.0",
    "react-native-screens": "~3.18.0",
    "react-native-is-iphonex": "^1.0.1",
    "react-native-web": "~0.18.7",
    "react-native-safe-area-context": "4.4.1"
  },
  "devDependencies": {
    "babel-preset-expo": "~9.2.2",
	"prettier": "^2.8.1",
	"eslint": "^8.32.0",
    "@react-native-community/eslint-config": "^3.2.0"
  },
  "private": true
}
